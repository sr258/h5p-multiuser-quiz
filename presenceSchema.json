{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Presence schema",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "level": {
      "enum": [
        "anonymous",
        "user",
        "privileged"
      ]
    }
  },
  "required": [
    "userId",
    "name",
    "level"
  ]
}