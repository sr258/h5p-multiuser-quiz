  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-destructuring": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@h5p-types/h5p-core": "^0.2.0",
    "@types/color": "^3.0.2",
    "@types/jquery": "^3.5.11",
